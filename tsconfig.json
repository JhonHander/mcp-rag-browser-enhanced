{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "build",
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "lib": ["ES2022"],
        "skipLibCheck": true,
        "strict": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": ["node_modules"]
}
